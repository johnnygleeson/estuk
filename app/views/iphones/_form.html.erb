<%= simple_form_for(@iphone) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :model %>
    <%= f.input :GBs %>
    <%= f.input :Memory %>
    <%= f.input :condition %>
    <%= f.input :price %>
    <%= f.input :availability %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
